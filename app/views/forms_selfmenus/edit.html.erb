<h1><%= t('Editing'.downcase) %> : <%= t('activerecord.models.forms_selfmenu') %></h1>

<% form_for(@forms_selfmenu) do |f| %>
  <%= f.error_messages %>

  <p>
     <%= f.label :selfmenu_id, t('activerecord.attributes.forms_selfmenu.selfmenu_id') %><br />
    <%= f.text_field :selfmenu_id %>
  </p>
  <p>
     <%= f.label :form_id, t('activerecord.attributes.forms_selfmenu.form_id') %><br />
    <%= f.text_field :form_id %>
  </p>
  <p>
     <%= f.label :remarks, t('activerecord.attributes.forms_selfmenu.remarks') %><br />
    <%= f.text_area :remarks %>
  </p>
  <p>
    <%= f.submit t('Update'.downcase) %>
  </p>
<% end %>

<%= link_to t('Show'.downcase), @forms_selfmenu %> |
<%= link_to t('Back'.downcase), forms_selfmenus_path %>
