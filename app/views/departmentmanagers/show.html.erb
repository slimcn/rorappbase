<h1><%= t('Show'.downcase) %> : <%= t('activerecord.models.departmentmanager') %></h1>

<p>
  <b><%= t('activerecord.attributes.departmentmanager.Department_id'.downcase) %>:</b>
  <%=h @departmentmanager.department.code+@departmentmanager.department.name %>
</p>

<p>
  <b><%= t('activerecord.attributes.departmentmanager.Employe_id'.downcase) %>:</b>
  <%=h @departmentmanager.employe.code+@departmentmanager.employe.name %>
</p>

<p>
  <b><%= t('activerecord.attributes.departmentmanager.Remarks'.downcase) %>:</b>
  <%=h @departmentmanager.remarks %>
</p>


<%= link_to t('Edit'.downcase), edit_departmentmanager_path(@departmentmanager) %> |
